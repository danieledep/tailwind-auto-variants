{
	"name": "tailwind-auto-variants",
	"displayName": "Tailwind Auto Variants",
	"description": "Auto generate variants for Tailwind CSS",
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Snippets"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/danieledep/tailwind-variants-snippets"
	},
	"publisher": "danieledep",
	"scripts": {
		"build": "vsce package",
		"publish": "vsce publish"
	},
	"icon": "images/icon.png",
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "snippets/html.json"
			},
			{
				"language": "css",
				"path": "snippets/css.json"
			},
			{
				"language": "javascriptreact",
				"path": "snippets/javascriptreact.json"
			},
			{
				"language": "typescriptreact",
				"path": "snippets/typescriptreact.json"
			},
			{
				"language": "liquid",
				"path": "snippets/liquid.json"
			},
			{
				"language": "twig",
				"path": "snippets/twig.json"
			}
		]
	}
}