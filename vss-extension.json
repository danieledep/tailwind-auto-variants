{
	"manifestVersion": 1,
	"id": "tailwind-auto-variants",
	"version": "0.0.2",
	"name": "Tailwind Auto Variants",
	"publisher": "danieledep",
	"description": "Smart snippets for Tailwind CSS variants",
	"repository": {
			"type": "git",
			"uri": "https://github.com/danieledep/tailwind-auto-variants"
	},
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "html",
				"path": "./html.json"
			},
			{
				"language": "css",
				"path": "./css.json"
			},
			{
				"language": "javascriptreact",
				"path": "./javascriptreact.json"
			},
			{
				"language": "typescriptreact",
				"path": "./typescriptreact.json"
			},
			{
				"language": "liquid",
				"path": "./liquid.json"
			}
		]
	},
	"icon": "images/icon.png"
}